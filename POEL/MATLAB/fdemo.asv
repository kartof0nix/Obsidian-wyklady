Fg = 48;

Hd = @(f) (-1/Fg) * (exp(-i*2*pi*Fg / f) / (1 - (exp(-i*2*pi*Fg / f))));
Ha = @(f) 1j ./ (2 * pi .* f);

Db = @(x) 20 * log(x);

Ha(1.3)
Ha(5.89)


xscale log
grid on

in0 = [0.1, 6];
in1 = [0.1, 24];
in2 = [0.1, 96];

tiledlayout(2,1)

nexttile
hold on
fplot(@(x) Db(abs(Ha(x))), in0);
fplot(@(x) Db(abs(Hz(x))), in0);
ylabel("|H(f) [dB]");
xlabel("F [kHz]");
legend("Ha(f)", "Hz(f)");
hold off


